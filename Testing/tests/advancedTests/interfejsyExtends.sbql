begin;
create interface IOsoba {object name is OIOsoba; (imie; nazwisko) zmienNazwisko(noweNazwisko);};
create interface IStudent extends IOsoba {object name is OIStudent; (rok; uczelnia) kiedyKonczy();};
create interface IPrac extends IOsoba {object name is OIPrac; (zarobki; firma) zmienZar(o);};
create interface IDzielnyStudent extends IStudent, IPrac {object name is OIDzielnyStudent; () dajMedalZKartofla();};

create class COsoba {instance OOsoba : {imie; nazwisko; wzrost; plec} procedure zmienNazwisko(noweNazwisko) {return 0} };
create class CStudent extends COsoba {instance OStudent : {rok; uczelnia; sredniaOcen} procedure kiedyKonczy() {return 0} procedure stypendium() {return 0} };
create class CPrac extends COsoba {instance OPrac : {zarobki; firma} procedure zmienZar(o) {return 0}};
create class CDzielnyStudent extends CStudent, CPrac {instance ODzielnyStudent: {dzielnosc} procedure dajMedalZKartofla() {return 0}};

interface IDzielnyStudent shows CDzielnyStudent;

CDzielnyStudent includes create (1 as dzielnosc, 20 as zarobki, "Szogunat Rolniczy" as firma, 3 as rok, "UW" as uczelnia, 4 as sredniaOcen, "arnold" as imie, "Bombel" as nazwisko, 177 as wzrost, "M" as plec) as ODzielnyStudent;

OIDzielnyStudent;


end;  
